# This is a sample .env file for use in local development.
# Duplicate this file as .env in the root of the project
# and update the environment variables to match your
# desired config

# Configuration of the app: name, version, and folder where to store uploaded and temporary files
APP_NAME="OTSunWebApp Personal Server"
VERSION='3.3.3'
UPLOAD_FOLDER = "/tmp/OTSunWebApp"

# Configuration of the host system: number of CPUs that can be used and port to map
CPU_COUNT=4
HOST='0.0.0.0'
PORT=8888

# Configuration of the mail server.
# If it is not configured, then user shall not know when the computation is finished and will have to watch the status page.
MAIL_SERVER=
MAIL_PORT=
MAIL_SENDER=
MAIL_USERNAME=
MAIL_PASSWD=
